<% if @load_more %>
  <%= javascript_tag do %>
    window.fetchMore = true;
  <% end %>
<% end %>

<h3>Available Products</h3>

<div class="products_grid row" data="<%= @products.last.created_at %>">
  <%= render partial: "product", collection: @products, as: :product %>
</div>

<div id="loading_txt">
  Loading ...
</div>
